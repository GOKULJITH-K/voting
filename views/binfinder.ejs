    let unoccupied = 100 - `<%= canteen %>`;
     var xValues = ["Occupied", "unoccupied"];
    var yValues1 = [`<%= canteen %>`, unoccupied];
    var barColors = ["rgb(221, 87, 70)", "rgb(65, 176, 110)"];
    
    new Chart("canteen", {
      type: "doughnut",
