   <script>
    var xValues = ["Occupied", "unoccupied"];
    var yValues = [`<%= nitrogen %>`, ];
    var barColors = ["red", "blue"];
    
    new Chart("barChart", {
      type: "bar",
      data: {
        labels: xValues,
        datasets: [{
          backgroundColor: barColors,
          data: yValues
        }]
      },
      options: {
        legend: {display: false},
        title: {
          display: true,
          text: "Visualization of Soil Sample - Bar Graph"
        }
      }
    });
    new Chart("doughChart", {
    type: "doughnut",
      data: {
        labels: xValues,
        datasets: [{
          backgroundColor: barColors,
          data: yValues
        }]
      },
      options: {
        legend: {display: false},
        title: {
          display: true,
          text: "Visualization of Soil Sample - Doughnut Graph"
        }
      }
    });
